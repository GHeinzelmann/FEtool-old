# Input variables for mouse Major Urinary Protein-I (MUP-I) - based on 1znk crystal structure


# Ligand anchor search definitions

l1_x            = 2.00     # x distance from H1 to center of L1 strike zone
l1_y            = -6.00    # y distance from H1 to center of L1 strike zone             
l1_z            = 8.50     # minimum z distance between H1 and L1
l1_range        = 3.00     # Initial search range (strike zone) of the first ligand anchor L1 
min_adis        = 3.40     # minimum distance between anchors
max_adis        = 8.00     # maximum distance between anchors

# Protein anchors, using original sequence 

H1 = :134@CA
H2 = :14@CA
H3 = :89@CA

